{
  "name": "s3-access-log-ingest",
  "source": {
    "schema": {
      "name": "aws-s3-access-log",
      "embedsSchema": false
    }
  },
  "transform": [
    "@{source.manifestLot}=>lot|string",
    "time+>time_ymd|DateTime|yyyyMMdd",
    "time_ymd|string"
  ],
  "sink": {
    "schema": {
      "format": "parquet",
      "compression": "gzip"
    },
    "partitions": [
      "time_ymd",
      "lot"
    ],
    "namedPartitions": true,
    "filename": {
      "prefix": "access",
      "includeGuid": true,
      "providedGuid": "@{sink.manifestLot}-@{currentTimeMillis}",
      "includeFieldsHash": true
    }
  }
}
